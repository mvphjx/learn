				</xs:annotation>
			</xs:element>
			<xs:element name="SiteLongitude" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<puabis:field length="12" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="SiteTemperature" type="xs:byte"
